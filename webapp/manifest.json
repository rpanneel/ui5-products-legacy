{
	"_version": "1.17.0",
	"sap.app": {
		"id": "be.rpan.epm.products",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZEPM_DEMO_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "be.rpan.epm.products.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "be.rpan.epm.products.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"defaultBindingMode": "TwoWay",
				"preload": true,
				"settings": {
					"useBatch": true,
					"defaultCountMode": "Inline",
					"json": true
				}
			}
		},
		"resources": {
			"css": [],
			"js": []
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "be.rpan.epm.products.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"name": "detail",
					"pattern": ":parent*:-/detail/{id}",
					"target": ["detail"]
				},
				{
					"name": "forSupplier",
					"pattern": ":parent*:-/productsForSupplier/{supplierId}",
					"target": ["list"]
				}
				{
					"name": "list",
					"pattern": ":parent*:",
					"target": ["list"]
				}
			],
			"targets": {
				"list": {
					"type": "View",
					"id": "list",
					"name": "List",
					"title": "Products List"
				},
				"detail": {
					"type": "View",
					"id": "detail",
					"name": "Detail",
					"title": "{Name}"
				},
				"notFound": {
					"type": "View",
					"id": "notFound",
					"name": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}
